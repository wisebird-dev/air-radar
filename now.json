{
    "version": 2,
    "alias": ["kepinski.me", "www.kepinski.me"],
    "builds": [
        { "src": "package.json", "use": "@now/static-build", "config": {"distDir": "public"} }
      ],
    "routes": [
      {
        "src": "/", 
        "headers": {
          "cache-control": "s-maxage=2628000, maxage=0",
          "accept-encoding": "br"
        }
      }
    ],
    "regions": ["all"]
}