{
  "name": "kepinski-gatsby",
  "description": "Personal site built with Gatsby, React & styled-components",
  "version": "1.0.0",
  "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
  "dependencies": {
    "gatsby": "^2.1.4",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-sharp": "^2.1.13",
    "modern-normalize": "^0.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3",
    "typeface-roboto-mono": "0.0.54"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-react": "^7.12.4",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "xo": "^0.24.0"
  },
  "keywords": [
    "kepinski"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "now-build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "test": "xo && stylelint 'src/**/*.js'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xxczaki/kepinski.me"
  },
  "bugs": {
    "url": "https://github.com/xxczaki/kepinski.me/issues"
  },
  "xo": {
    "nodeVersion": ">=10",
    "parser": "babel-eslint",
    "envs": [
      "node",
      "browser"
    ],
    "extends": "xo-react",
    "settings": {
      "react": {
        "version": "16.8"
      }
    },
    "rules": {
      "import/no-unassigned-import": "off"
    }
  }
}
