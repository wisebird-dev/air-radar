{
  "name": "kepinski-react",
  "version": "1.0.0",
  "description": "Personal Website built with React & styled-components",
  "scripts": {
    "start": "webpack-dev-server --hot --open --mode development",
    "now-build": "npm test && webpack --mode production",
    "test": "xo && stylelint './src/**/*.js' && jest"
  },
  "main": "src/index.js",
  "repository": "https://github.com/xxczaki/kepinski.me",
  "author": "Antoni Kepinski",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0 ",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "imagemin": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^6.0.0",
    "img-loader": "^3.0.1",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "react-test-renderer": "^16.6.1",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "stylelint": "^9.8.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.0",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "xo": "^0.23.0"
  },
  "dependencies": {
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "styled-components": "^4.0.3",
    "webfontloader": "^1.6.28"
  },
  "xo": {
    "extends": "xo-react",
    "envs": "browser",
    "esnext": true,
    "ignores": [
      "src/test",
      "src/index.js"
    ],
    "rules": {
      "react/no-unescaped-entities": null
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
