{
  "name": "kepinski-nextjs",
  "version": "1.0.0",
  "description": "Personal Website built with Next.js, React & styled-components",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "xo && stylelint './pages/**/*.js' && jest"
  },
  "main": "pages/index.js",
  "repository": "https://github.com/xxczaki/kepinski.me",
  "author": "Antoni Kepinski <a@kepinski.me> (https://kepinski.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-plugin-styled-components": "^1.10.0",
    "eslint-config-xo-react": "^0.18.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.6.0",
    "jest-styled-components": "^6.3.1",
    "react-test-renderer": "^16.7.0",
    "stylelint": "^9.10.1",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.2",
    "xo": "^0.24.0"
  },
  "dependencies": {
    "fontfaceobserver": "^2.1.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "xo": {
    "nodeVersion": ">=10",
    "parser": "babel-eslint",
    "envs": [
      "node",
      "browser",
      "jest"
    ],
    "extends": "xo-react",
    "settings": {
      "react": {
        "version": "16.7"
      }
    },
    "rules": {
      "import/no-unassigned-import": "off",
      "unicorn/filename-case": "off",
      "react/jsx-sort-props": "off",
      "react/prop-types": "off"
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
