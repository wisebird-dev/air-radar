    "repository": {
        "type": "git",
        "url": "git+https://github.com/xxczaki/kepinski.me.git"
    },
    "bugs": {
        "url": "https://github.com/xxczaki/kepinski.me/issues"
    },
    "xo": {
        "nodeVersion": ">=10",
        "parser": "babel-eslint",
        "envs": [
            "node",
            "browser",
            "jest"
        ],
        "extends": "xo-react",
        "settings": {
            "react": {
                "version": "16.11"
            }
        },
        "rules": {
            "import/no-unassigned-import": "off",
            "jsx-a11y/anchor-is-valid": "off"
        }
    },
    "homepage": "https://github.com/xxczaki/kepinski.me#readme"
